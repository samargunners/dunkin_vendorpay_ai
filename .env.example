# VendorPay AI Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Database Configuration (if using direct PostgreSQL connection)
DATABASE_URL=postgresql://username:password@localhost:5432/vendorpay_ai

# Application Configuration
ENVIRONMENT=development
SECRET_KEY=your_secret_key_here
LOG_LEVEL=INFO

# Document Storage Configuration
MAX_FILE_SIZE_MB=50
ALLOWED_FILE_TYPES=pdf,csv,xlsx,xls,jpg,jpeg,png,tiff

# OCR Configuration (Tesseract)
TESSERACT_CMD=/usr/bin/tesseract  # Linux/Mac
# TESSERACT_CMD=C:\Program Files\Tesseract-OCR\tesseract.exe  # Windows

# AI/ML Configuration
MODEL_CONFIDENCE_THRESHOLD=0.7
AUTO_RECONCILE_THRESHOLD=0.9

# Notification Configuration
ENABLE_EMAIL_NOTIFICATIONS=false
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# API Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Security
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]